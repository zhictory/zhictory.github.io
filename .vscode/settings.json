{
  "markdown.styles": [".attachments/markdown-theme/juejin.css"],
  "[markdown]": {
    "editor.fontFamily": "'Cascadia Code NF', '霞鹜文楷', 'Source Han Serif CN'",
    "editor.lineHeight": 1.5,
    "editor.fontSize": 12,
    "editor.wordWrap": "on",
    "editor.formatOnSave": false,
    "editor.renderWhitespace": "all",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    },
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "on"
  },
  "markdown.preview.fontFamily": "'Cascadia Code NF', '霞鹜文楷', 'Source Han Serif CN'",
  "markdown.preview.lineHeight": 1.5,
  "markdown.preview.fontSize": 16,
  "markdown.preview.doubleClickToSwitchToEditor": true
}
